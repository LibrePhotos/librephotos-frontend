{
  "name": "librephotos-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:warn": "eslint --cache \"src/**\"",
    "lint:error": "eslint --cache --quiet \"src/**\"",
    "lint:error:fix": "eslint --quiet \"src/**\" --fix",
    "lint:warning:fix": "eslint --cache --quiet \"src/**\" --fix",
    "start": "react-scripts start",
    "build": "rimraf ./build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "eslint-fix-interactive": "eslint-nibble --fixable-only \"./src/**/*\"",
    "eslint-fix-specified": "eslint-filtered-fix \"./src/**/*\" --rule import/no-duplicates"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn prettier --config ./prettier.config.js  --write",
      "yarn eslint --cache --quiet -c ./.eslintrc.commitHook.js"
    ],
    "*.{json,css,scss,md}": [
      "yarn prettier --config ./prettier.config.js  --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
